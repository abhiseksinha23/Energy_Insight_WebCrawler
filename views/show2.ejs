    <a href="/crawler">CRAWLER</a>
  <table style="width:97%;margin:1em;">
    <thead>
      <tr>
        <th>SYMBOL</th>
      <th>NAME</th>
      <th>LAST PRICE</th>
        <th>MARKET TIME</th>
      <th>CHANGE</th>
      <th>%CHANGE</th>
      <th>VOLUME</th>
      <!-- <th>OPEN INTEREST</th> -->
        </tr>
      <thead>
      <tbody>

    <% data.forEach((element)=>{ %>
      <tr>
        <td><%=element.sym%></td>
        <td><%=element.name%></td>
        <td><%=element.price%></td>
        <td><%=element.markettime%></td>
        <td><%=element.change %></td>
        <td><%=element.changePer%></td>
        <td><%=element.volume%></td>
        <!-- <td><%=element.openInterest%></td> -->
      </tr>
  <%  });%>

    </tbody>
  </table>
