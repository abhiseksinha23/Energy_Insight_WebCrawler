      <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" style="background-color:#636363;color:#ffffff;">
          <h3 style="text-align:center;"><u>TODAY'S WORLD TRADE MARKET PRICE</u></h3>
        <div class="carousel-inner" style="text-align:center;">
                <div class="carousel-item active">
                   <div class="row">
                <%for(let j=0; j<=3; j++) {%>
                  <div class = "col-sm-3">
                      <%let element = data[j];%>
                        <!-- <%=element.sym%><br> -->
                  <%=element.name%><br>
                  <%=element.price%><br>
                  <!-- <%=element.markettime%><br> -->
                  <%=element.change %><br>
                  <%=element.changePer%><br>
                  <!-- <%=element.volume%><br> -->
                  <!-- <td><%=element.openInterest%></td> -->
                </div>
                  <% } %>
                </div>
                  </div>
                  <% let count=4; %>
                  <%for(let i=0; i<=3; i++){ %>
                  <div class="carousel-item">
                      <div class="row">
                    <%for(let j=count; j<=count+3; j++){ %>
                      <div class = "col-sm-3">
                          <%let element = data[j];%>
                          <!-- <%=element.sym%><br> -->
                      <%=element.name%><br>
                      <%=element.price%><br>
                      <!-- <%=element.markettime%><br> -->
                      <%=element.change %><br>
                      <%=element.changePer%><br>
                      <!-- <%=element.volume%><br> -->
                      <!-- <td><%=element.openInterest%></td> -->
                    </div>
                      <% } %>
                    </div>
                      </div>
                      <% count+=4; %>
                    <% } %>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
