              <%=data[j].price%><br>
                <%=data[j].change%><br>
                  <%=data[j].changePer%>
    </div>
      <% } %>
    </div>
      </div>
      <% let count=4; %>
      <%for(let i=0; i<=3; i++){ %>
      <div class="carousel-item">
          <div class="row">
        <%for(let j=count; j<=count+3; j++){ %>
          <div class = "col-sm-3">
              <%let element = data[j];%>
            <strong><i> <%=data[j].name%></i></strong><br>
                <%=data[j].price%><br>
                  <%=data[j].change%><br>
                    <%=data[j].changePer%><br>
        </div>
          <% } %>
        </div>
          </div>
          <% count+=4; %>
          <% } %>

<% } %>
